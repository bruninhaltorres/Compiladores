function int _fibonacci(int _n) {
    if( _n < 2 ) {
        return _n;    
    }

    return _fibonacci(_n - 1) + _fibonacci(_n - 2);
}

function main() {
    
    int _n, _total;
    SysOut("Digite o nÃºmero limite desejado");
    SysIn(_n);

    int _i;
    for(_i = 0, _n) {
        _total = _fibonacci(_i);
        if(_i == _n - 1) {
            SysOut(_total);
        } else {
            SysOut(_total + ", ");
        }
    }
}