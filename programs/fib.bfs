function int _fibonacci(int _n) {
    if( _n < 2 ) {
        return _n;    
    }

    return _fibonacci(_n - 1) + _fibonacci(_n - 2);
}

function void main() {
    
    int _n, _total;
    SysOut("Digite o tamanho da sequencia:");
    SysIn(_n);

    int _i;
    for(_i = 0, _i < _n) {
        _total = _fibonacci(_i);
        SysOut(_total);
    }
}